#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules/fumadocs-mdx/dist/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules/fumadocs-mdx/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules/fumadocs-mdx/dist/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules/fumadocs-mdx/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/fumadocs-mdx@13.0.0_fumadocs-core@16.0.2_@types+react@19.2.2_next@16.0.7_@babel+core@7._71a14597311a64a5ceba6e297affc8d3/node_modules:/Users/hex/Documents/GitHub/nocta-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
else
  exec node  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
fi
